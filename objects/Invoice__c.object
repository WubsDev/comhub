{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

var invoiceIds= {!GETRECORDIDS($ObjectType.Invoice__c)};
try{ 
     if(invoiceIds != &apos;&apos;){
          var result = sforce.apex.execute(&quot;MassUpdateInvoice&quot;, &quot;massUpdate&quot;,{invoiceId:invoiceIds});
          window.location.reload();
          alert(&quot;Status Updated Successfully - &quot;+ result );
	}
	else{
		alert(&quot;Please select the records&quot;);
	}
}
catch(err) { 
